{
  "name": "vue3-test-shell",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode localhost",
    "build": "run-s type-check \"build-prod-only {@}\" --",
    "build-dev": "run-s type-check \"build-dev-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build --mode production",
    "build-prod-only": "vite build --mode production",
    "build-dev-only": "vite build --mode development",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@formwerk/core": "^0.7.0",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "axios": "^1.7.7",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "pinia": "^2.1.7",
    "tailwind-merge": "^2.5.3",
    "vee-validate": "^4.14.4",
    "vue": "^3.4.29",
    "vue-i18n": "^10.0.4",
    "vue-router": "^4.3.3",
    "vue-the-mask": "^0.11.1",
    "vue-toastification": "^2.0.0-rc.5",
    "vue3-icon": "^3.0.3",
    "yup": "^1.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/js-cookie": "^3.0.6",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.15",
    "@types/node": "^20.14.5",
    "@types/vue-the-mask": "^0.11.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "cross-env": "^7.0.3",
    "cypress": "^13.12.0",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^24.1.0",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.4.47",
    "prettier": "^3.2.5",
    "start-server-and-test": "^2.0.4",
    "tailwindcss": "^4.0.0",
    "typescript": "~5.4.0",
    "vite": "^5.3.1",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.21"
  }
}
